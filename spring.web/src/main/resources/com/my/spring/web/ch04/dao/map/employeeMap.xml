<?xml version='1.0' encoding='utf-8' ?>
<!DOCTYPE mapper PUBLIC '-//mybatis.org//DTD Mapper 3.0//EN'
	'http://mybatis.org/dtd/mybatis-3-mapper.dtd'>
<mapper namespace='com.my.spring.web.ch04.dao.map.EmployeeMap'>
	<select id='selectEmployee' resultType='com.my.spring.web.ch04.domain.Employee'>
		select employee_id employeeId, last_name lastName, hire_date hireDate
		from employees
		where employee_id = #{employeeId}
	</select>
</mapper>
<!-- 
	resultType: 매핑될 클래스 명을 작성하는것 
	resultMap: 사용자가 원하는 POJO클래스(VO) 매핑
-->